{
  "expo": {
    "name": "lumiere",
    "slug": "lumiere",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/dark-logo.png",
    "splash": {
      "image": "./assets/loading_sablier.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.em.lumiere",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/dark-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon-logo.png"
    }
  }
}
